{
  "name": "portfolio-manager-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Portfolio Manager Monorepo",
  "scripts": {
    "dev:webapp": "npm run -w frontend/webapp start",
    "build:webapp": "npm run -w frontend/webapp build",
    "test:webapp": "npm run -w frontend/webapp test",
    "install:all": "npm install",
    "clean": "rm -rf node_modules package-lock.json && rm -rf frontend/webapp/node_modules frontend/webapp/package-lock.json && rm -rf backend/node_modules backend/package-lock.json",
    "deploy": "npm run deploy:production",
    "deploy:production": "./scripts/deploy-production.sh",
    "deploy:preview": "./scripts/deploy-preview.sh",
    "deploy:force": "bash ./scripts/deploy-production.sh",
    "e2e": "./scripts/run-e2e-tests.sh all",
    "e2e:aws": "./scripts/run-e2e-tests.sh aws",
    "e2e:performance": "./scripts/run-e2e-tests.sh performance",
    "e2e:ui": "./scripts/run-e2e-tests.sh ui",
    "e2e:debug": "./scripts/run-e2e-tests.sh debug",
    "e2e:report": "./scripts/run-e2e-tests.sh report"
  },
  "workspaces": {
    "packages": [
      "frontend/webapp",
      "backend"
    ],
    "nohoist": [
      "**/react-scripts",
      "**/react-scripts/**",
      "**/check-types",
      "**/check-types/**",
      "**/webpack",
      "**/webpack/**",
      "**/babel-loader",
      "**/babel-loader/**",
      "**/@babel/**",
      "**/eslint",
      "**/eslint/**"
    ]
  },
  "overrides": {
    "cookie": "^0.7.0",
    "cross-spawn": "^7.0.5",
    "follow-redirects": "^1.15.6",
    "nanoid": "^3.3.8",
    "protobufjs": "^7.2.5",
    "tar": "^6.2.1",
    "word-wrap": "^1.2.5",
    "@grpc/grpc-js": "^1.8.22"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
